{
    "manifest_version": 2,
    "name": "Focus Simple",
    "version": "0.2.2",
    "permissions": [
        "activeTab",
        "storage",
        "contextMenus"
    ],
    "background": {
        "scripts": [
            "background.js"
        ]
    },
    "content_scripts": [
        {
            "matches": [
                "<all_urls>"
            ],
            "js":[
                "content.js",
                "jquery-3.3.1.min.js"
            ]
        }
    ],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "icons": {
        "16": "icon16.png",
        "128": "icon128.png"
    }
}
